{
  "expo": {
    "name": "Smartfarm",
    "slug": "mqtt-demo",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/icon-app.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "./build/withCleartextTraffic"
      ],
      [
        "expo-font",
        {
          "Montserrat-Bold": ["./src/assets/fonts/Montserrat-Bold.ttf"]
        }
      ]
    ],
    "splash": {
      "image": "./src/assets/splash-neww.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#bfd7eb"
      },
      "package": "com.ifanhaq.smartfarm_mobile_app",
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4774c083-5df4-4456-938f-4cb35b5c9a4b"
      }
    }
  }
}
